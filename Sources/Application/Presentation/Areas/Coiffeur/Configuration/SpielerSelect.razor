@using JassApp.Domain.Spieler.Models
@using MudBlazor
<MudSelect Value="SelectedSpieler"
           T="Spieler"
           ValueChanged="HandleSpielerChangedAsync"
           Variant="Variant.Outlined"
           Label="@Label"
           FitContent="false"
           Placeholder="Spieler auswählen">
    @foreach (var spieler in Spieler)
    {
        <MudSelectItem Value="spieler">@spieler.Name</MudSelectItem>
    }
</MudSelect>