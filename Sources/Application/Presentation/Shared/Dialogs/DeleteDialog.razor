@using MudBlazor
<MudDialog>
    <DialogContent>
        <MudText>Sind Sie sicher, dass Sie löschen wollen?</MudText>
    </DialogContent>
    <DialogActions>
        <MudButton OnClick="Cancel">Abbrechen</MudButton>
        <MudButton Color="@Color.Default" Variant="Variant.Filled" OnClick="Submit">Löschen</MudButton>
    </DialogActions>
</MudDialog>
